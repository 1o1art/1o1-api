    "start": "next start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1o1art/1o1art.git"
  },
  "author": "Shane Jonas, Zane Starr",
  "bugs": {
    "url": "https://github.com/1o1art"
  },
