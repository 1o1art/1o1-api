{
  "name": "@1o1art/sdk",
  "version": "0.0.0-development",
  "private": false,
  "homepage": "https://1o1.art",
  "description": "1of1 creation on-chain NFTs with delegation cli preview",
  "main": "build/index.js",
  "scripts": {
    "compile": "npx tsc",
    "build": "npm run gen && npm run compile",
    "gen": "rm src/artifacts/contracts/facets/**/*.dbg.json; typechain --target ethers-v5 --out-dir src/generated/typechain src/artifacts/contracts/facets/**/*.json",
    "start": "next start",
    "test": "npm run build",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/1o1art/1o1-sdk.git"
  },
  "author": "Zane Starr",
  "bugs": {
    "url": "https://github.com/1o1art/1o1-sdk"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.4.3",
    "@types/lodash": "^4.14.178",
    "@types/mime-types": "^2.1.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.11.19",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "chai": "^4.3.7",
    "eslint-config-prettier": "^8.5.0",
    "ethers": "^5.5.3",
    "fs-extra": "^0.30.0",
    "jest": "^29.5.0",
    "prettier": "^2.3.2",
    "prettier-eslint-cli": "^7.1.0",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "semantic-release": "^21.0.2",
    "standard": "^16.0.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.8.4",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint-webpack-plugin": "^3.1.1",
    "eslint": "^8.3.0"
  },
  "dependencies": {
    "@1o1art/1o1-contracts": "^1.0.3",
    "@typechain/ethers-v5": "^10.2.0",
    "@erc725/erc725.js": "^0.17.2",
    "autoprefixer": "^10.4.12",
    "bufferutil": "^4.0.7",
    "lodash": "^4.17.21",
    "mime": "^1.6.0",
    "mime-types": "^2.1.35",
    "utf-8-validate": "^5.0.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "build/**/*"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "resetMocks": true
  },
  "eslintConfig": {},
  "publishConfig": {
    "access": "public"
  }
}
