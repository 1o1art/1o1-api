    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@erc725/erc725.js": "^0.17.2",
    "@etclabscore/eserialize": "^1.1.2",
